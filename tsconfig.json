{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "ES5",
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types", "src/types"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
